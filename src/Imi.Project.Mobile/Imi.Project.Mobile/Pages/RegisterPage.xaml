<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Imi.Project.Mobile.Pages.RegisterPage">
    <ContentPage.Content>
        <StackLayout>
            <Label>Register</Label>
            <Entry Placeholder="Username"></Entry>
            <Entry Placeholder="Email"></Entry>
            <Entry IsPassword="True" Placeholder="Password"></Entry>
            <Entry IsPassword="True" Placeholder="Confirm Password"></Entry>
            <Button Text="Register"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>